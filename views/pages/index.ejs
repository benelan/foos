<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div class="jumbotron">
        <p><%= tagline %></p>
        <ul>
            <% table.forEach(function(t) { %>
                <li><%= t.name %> - <%= t.points %></li>
            <% }); %>
        </ul>
        <p><%= data %></p>
    </div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>